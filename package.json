{
  "name": "js-dev-environment",
  "version": "1.0.0",
  "description": "JavaScript development environment",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel security-check open:src lint:watch test:watch start-mockapi",
    "open:src": "babel-node buildScripts/srcServer.js",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "security-check": "nsp check",
    "localtunnel": "lt --port 3000 --subdomain danielandrews",
    "share": "npm-run-all --parallel open:src localtunnel",
    "test": "mocha buildScripts/testSetup.js \"src/**/*.test.js\" --reporter progress",
    "test:watch": "npm run test -- --watch",
    "test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root src --report lcovonly _mocha -- --require buildScripts/testSetup.js \"src/**/*.test.js\" && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "test:coverage": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec --require buildScripts/testSetup.js \"src/**/*.test.js\" && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js",
    "deploy": "surge ./dist"
  },
  "author": "Daniel Andrews",
  "license": "MIT",
  "dependencies": {
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "8.0.4",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-preset-latest": "6.24.1",
    "babel-register": "6.26.0",
    "chai": "4.2.0",
    "chalk": "2.4.1",
    "cheerio": "0.22.0",
    "compression": "1.7.3",
    "coveralls": "^3.0.2",
    "cross-env": "^6.0.3",
    "css-loader": "3.5.3",
    "eslint": "6.5.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-watch": "6.0.1",
    "express": "4.16.4",
    "extract-text-webpack-plugin": "3.0.2",
    "html-webpack-plugin": "3.2.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.5",
    "jsdom": "15.2.0",
    "json-schema-faker": "0.4.7",
    "json-server": "0.14.0",
    "localtunnel": "1.9.1",
    "mocha": "6.2.2",
    "mocha-webpack": "^1.1.0",
    "nock": "11.6.0",
    "npm-run-all": "4.1.5",
    "nsp": "3.2.1",
    "numeral": "2.0.6",
    "nyc": "^13.1.0",
    "open": "7.0.0",
    "rimraf": "2.6.2",
    "style-loader": "0.23.1",
    "surge": "^0.20.1",
    "webpack": "4.41.2",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "2.24.3",
    "webpack-md5-hash": "0.0.6"
  }
}
